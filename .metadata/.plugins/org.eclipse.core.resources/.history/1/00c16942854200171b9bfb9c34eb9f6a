package javahacker;

public class CarTruckMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Truck cartruck =new Truck(null, 0, 0, 0);
		cartruck.Model=2016;
		cartruck.MilesDriven=100;
		cartruck.SetHasTruck(true);
		cartruck.Price=23300;
		cartruck.Type="GSM";

		
		Truck cartruck2 =new Truck(null, 0, 0, 0);
		cartruck2.Model=2015;
		cartruck2.MilesDriven=50;
		cartruck2.SetHasTruck(false);
		cartruck2.Price=13300;
		cartruck2.Type="poo";
		

		}


}
