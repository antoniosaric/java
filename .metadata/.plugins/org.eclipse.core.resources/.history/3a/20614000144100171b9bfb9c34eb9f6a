package javahacker;

	import java.util.*;

	public class appleOrange {

	    private static Scanner in;

		public static void main(String[] args) {
	        in = new Scanner(System.in);
	        int s = in.nextInt();
	        int t = in.nextInt();
	        int a = in.nextInt();
	        int b = in.nextInt();
	        int m = in.nextInt();
	        int n = in.nextInt();
	        int apple_count = 0;
	        int orange_count = 0;
	        
	        int[] apple = new int[m];
	        for(int apple_i=0; apple_i < m; apple_i++){
	            apple[apple_i] = in.nextInt();
	        }
	        int[] orange = new int[n];
	        for(int orange_i=0; orange_i < n; orange_i++){
	            orange[orange_i] = in.nextInt();
	        }
	        
	        for (int i = 0; i < apple.length; i++){
	            int close = s - a;
	            if(close < 0){
	            	Math.abs(close);
	            }
	            int far = t - a ;
	            if(far < 0){
	            	Math.abs(far);
	            }

	            if ( apple[i] >= close && apple[i] <= far && apple[i] > 0){
	                apple_count++;
	            }
	        }
	        	        
	        for(int i = 0; i < orange.length; i++){
	            int close = s - b;
	            if(close < 0){
	            	Math.abs(close);
	            }
	            int far = t - b ;
	            if(close < 0){
	            	Math.abs(close);
	            }
	            
	            if (orange[i] > close && orange[i] < far && orange[i] < 0){
		            
	        
	            	System.out.println("close");
        			System.out.println(close);
        			System.out.println("far");
        			System.out.println(far);
            		
	                orange_count++;
        			System.out.println(orange_count);

	            }
	        }
	        
	        System.out.println(apple_count);
	        System.out.println(orange_count);
	   
	    }
	}
