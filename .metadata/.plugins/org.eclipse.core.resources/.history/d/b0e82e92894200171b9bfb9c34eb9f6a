package javahacker;

public class Truck extends Car{



	private boolean HasTruck;
	
	public void SetHasTruck(boolean HasTruck){
		this.HasTruck=HasTruck;
	}

	public Truck(String Type, int Model, double Price, double MilesDriven) {
		super(Type, Model, Price, MilesDriven);
		boolean HasTruck;
		// TODO Auto-generated constructor stub
		
		System.out.print(super.GetPrice());
	}
	@Override
	double GetPrice(){
		double NewPrice=Price-(MilesDriven*100);
		return NewPrice;
		
	}
	
	

}
