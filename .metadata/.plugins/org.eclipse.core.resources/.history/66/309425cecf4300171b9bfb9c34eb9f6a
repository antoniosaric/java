public class connectToDb {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Connection c = null;
			try{
				Class.forName("org.sqlite.JDBC");
				c=DriverManager.getConnection("test.db");
			}catch (Exception ex){
				System.out.println("cannot connect");
			}
			
			
			
			
			
	}

}

